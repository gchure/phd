@article{klauck2001,
  ids = {klauck2001a},
  title = {Role of the Response Regulator {{RssB}} in Sigma Recognition and Initiation of Sigma Proteolysis in {{{\emph{Escherichia}}}}{\emph{ Coli}}},
  author = {Klauck, E. and Lingnau, M. and {Hengge-Aronis}, R.},
  year = {2001},
