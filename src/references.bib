  language = {en},
  number = {13},
  pmid = {12805558}
}

@article{shabala2009,
  ids = {shabala2009a},
  title = {Ion Transport and Osmotic Adjustment in {\emph{Escherichia Coli}} in Response to Ionic and Non-Ionic Osmotica},
  author = {Shabala, L. and Bowman, J. and Brown, J. and Ross, T. and McMeekin, T. and Shabala, S.},
  year = {2009},
  month = jan,
  volume = {11},
  pages = {137--48},
  issn = {1462-2920 (Electronic) 1462-2912 (Linking)},
  doi = {EMI1748\\\%0020[pii]\\\%002010.1111/j.1462-2920.2008.01748.x},
  abstract = {Bacteria respond to osmotic stress by a substantial increase in the intracellular osmolality, adjusting their cell turgor for altered growth conditions. Using \emph{Escherichia coli} as a model organism we demonstrate here that bacterial responses to hyperosmotic stress specifically depend on the nature of osmoticum used. We show that increasing acute hyperosmotic NaCl stress above approximately 1.0 Os kg(-1) causes a dose-dependent K(+) leak from the cell, resulting in a substantial decrease in cytosolic K(+) content and a concurrent accumulation of Na(+) in the cell. At the same time, isotonic sucrose or mannitol treatment (non-ionic osmotica) results in a gradual increase of the net K(+) uptake. Ion flux data are consistent with growth experiments showing that bacterial growth is impaired by NaCl at the concentration resulting in a switch from net K(+) uptake to efflux. Microarray experiments reveal that about 40\% of upregulated genes shared no similarity in their responses to NaCl and sucrose treatment, further suggesting specificity of osmotic adjustment in \emph{E. coli} to ionic and non-ionic osmotica. The observed differences are explained by the specificity of the stress-induced changes in the membrane potential of bacterial cells highlighting the importance of voltage-gated K(+) transporters for bacterial adaptation to hyperosmotic stress.},
  journal = {Environmental Microbiology},
  keywords = {*Adaptation,<i>Escherichia coli</i>/*drug effects/*physiology,Anti-Bacterial Agents/*pharmacology,Gene Expression Profiling,Hypertonic/*pharmacology,Ion Transport,Membrane Potentials/drug effects/physiology,Oligonucleotide Array Sequence Analysis,Osmotic Pressure,Physiological,Potassium/metabolism,Saline Solution,Sucrose/pharmacology},
  language = {English}
}

@article{shahab1996,
  title = {Growth Rate Control of Protein and Nucleic Acid Content in {{Streptomyces}} Coelicolor {{A3}}(2) and {{Escherichia}} Coli {{B}}/r},
  author = {Shahab, Neelam and Flett, Fiona and Oliver, Stephen G. and Butler, Philip R.},
