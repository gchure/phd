@article{poelwijk2011b,
  title = {Tradeoffs and {{Optimality}} in the {{Evolution}} of {{Gene Regulation}}},
  author = {Poelwijk, Frank J. and {de Vos}, Marjon G. J. and Tans, Sander J.},
